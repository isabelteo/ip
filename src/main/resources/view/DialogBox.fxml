<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.text.Font?>
<?import javafx.scene.effect.Blend?>

<fx:root alignment="TOP_RIGHT" maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" minHeight="-Infinity" prefWidth="400.0" type="javafx.scene.layout.HBox" xmlns="http://javafx.com/javafx/11.0.0" xmlns:fx="http://javafx.com/fxml/1">
<children>
    <HBox fx:id="speechBubble" maxHeight="-Infinity" styleClass="speech-bubble" stylesheets="@application.css">
        <children>
            <Label fx:id="dialog" text="Label" textFill="BLACK" wrapText="true">
                <padding>
                    <Insets bottom="10.0" left="15.0" right="15.0" top="10.0" />
                </padding>
                <font>
                    <Font size="12.0" />
                </font>
            </Label>
        </children>
        <HBox.margin>
            <Insets left="5.0" right="5.0" />
        </HBox.margin>
    </HBox>
    <ImageView fx:id="displayPicture" fitHeight="100.0" fitWidth="100.0" pickOnBounds="true" preserveRatio="true">
        <effect>
            <Blend />
        </effect></ImageView>
</children>
<padding>
    <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
</padding>
</fx:root>